{
  "name": "fastify-qwik",
  "version": "0.1.0",
  "description": "Fastify plugin for Qwik City",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/*.ts --format cjs,esm --dts --clean --minify",
    "test": "vitest",
    "lint": "eslint ./src --ext .ts",
    "prepare": "pnpm lint --fix && pnpm build"
  },
  "dependencies": {
    "@builder.io/qwik-city": "^0.2.1",
    "fastify-plugin": "^4.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.13.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "keywords": [
    "fastify",
    "qwik"
  ],
  "author": "Matteo Pietro Dazzi",
  "license": "MIT"
}
